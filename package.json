{
  "name": "quizapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/art": "^1.2.0",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@testing-library/react-native": "^11.5.4",
    "@testing-library/user-event": "^14.4.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "jsdom-jscore-rn": "^0.1.8",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-native-draggable": "^3.3.0",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-pie-chart": "^2.1.1",
    "react-native-reanimated": "^2.14.4",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-select-dropdown": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@cucumber/cucumber": "^9.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@tsconfig/react-native": "^2.0.2",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^29.2.1",
    "@types/jsdom": "^21.1.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "cucumber": "^6.0.7",
    "eslint": "^8.19.0",
    "jest": "^29.5.0",
    "jest-cucumber": "^3.0.1",
    "jsdom": "^21.1.0",
    "jsdom-global": "^3.0.2",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "cacheDirectory": "./cache",
    "coveragePathIgnorePatterns": [
      "./app/utils/vendor"
    ],
    "testMatch": [
      "**/features/step_definitions/**/*.steps.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80
      }
    },
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@react-native|react-native|react-clone-referenced-element|react-navigation|react-native-gesture-handler|react-native-draggable|react-native-pie-chart|uuid|jest-cucumber)"
    ]
  }
}
