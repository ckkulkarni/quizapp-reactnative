var contains = function contains(item, searchTxt) {
  if (typeof item == 'object' && item != null) {
    for (var key in item) {
      var value = item[key];
      if (contains(value, searchTxt)) {
        return true;
      }
    }
  }
  if (typeof item != 'object' && item != null && item != undefined) {
    var itemStringfied = item.toString().toLowerCase();
    var searchTxtStringfied = searchTxt.toString().toLowerCase();
    if (itemStringfied.includes(searchTxtStringfied)) {
      return true;
    }
  }
  return false;
};
var deepSearchInArr = function deepSearchInArr(query, arr) {
  var array = [];
  for (var i = 0; i <= arr.length - 1; i++) {
    if (contains(arr[i], query)) {
      array.push(arr[i]);
    } else {
      array.push(null);
    }
    if (i == arr.length - 1) {
      return array;
    }
  }
};
exports.deepSearchInArr = deepSearchInArr;
